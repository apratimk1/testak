[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/apratimk1
[access "refs/for/refs/*"]
	read = group user/apratimk1
	push = group user/apratimk1
	label-Code-Review = -2..+2 group user/apratimk1
	label-Verified = -1..+1 group user/apratimk1
	rebase = group user/apratimk1
	removeReviewer = group user/apratimk1
	submit = group user/apratimk1
[access "refs/heads/*"]
	read = group user/apratimk1
	create = group user/apratimk1
	push = +force group user/apratimk1
	pushMerge = group user/apratimk1
[access "refs/tags/*"]
	push = group user/apratimk1
	push = +force group user/apratimk1
